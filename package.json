{
   "name": "simple_app",
   "version": "1.0.0",
   "description": "project",
   "main": "index.js",
   "repository": "github",
   "scripts": {
      "watch": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node src/server.ts",
      "build-ts": "tsc",
      "tslint": "tslint --fix -c tslint.json -p tsconfig.json",
      "serve": "node dist/server.js",
      "build": "npm run tslint && npm run build-ts",
      "start": "ts-node src/index.ts",
      "test-cucumber": "./node_modules/.bin/cucumber-js --parallel 1 --require ./features/steps/*.ts --require-module ts-node/register --format-options '{\"snippetInterface\": \"async-await\"}' --format json:reports/cucumber-report.json --format summary",
      "report": "node reports/cucumber-html-reporter.js"
   },
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@types/es6-promise": "^3.3.0",
      "@types/node": "^12.7.4",
      "koa": "^2.11.0",
      "koa-logger": "^3.2.1",
      "koa-router": "^7.4.0",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "request-promise": "^4.2.5",
      "tslint": "^5.20.1",
      "typeorm": "0.2.22"
   },
   "devDependencies": {
      "chai": "^4.2.0",
      "mocha": "^6.2.2",
      "nodemon": "^2.0.2",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333",
      "@types/node": "^8.0.29"
   }
}
